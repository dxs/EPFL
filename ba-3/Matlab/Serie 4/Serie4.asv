%interpol
x = 0:pi/100:3*pi;
f = @(x) sin(x) + x;

figure('Name', 'Exo 1');
plot(x,f(x));

hold on;
grid on;
Interpo(f,0,3*pi,2);
Interpo(f,0,3*pi,3);
Interpo(f,0,3*pi,4);
Interpo(f,0,3*pi,5);
Interpo(f,0,3*pi,6);

lgd = legend('sin(x) + x', 'n = 2','n = 3','n = 4', 'n = 5', 'n = 6');
legend('Location', 'best');
hold off;

figure('Name, 'Exo 2');
x = 0:3*pi/1001:3*pi;
f = @(x) sin(x);

